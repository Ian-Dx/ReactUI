{
  "name": "@iandx/reactui",
  "version": "1.0.0",
  "description": "A react add-on to write React just like SwiftUI.",
  "author": "Ian Duan",
  "license": "MIT",
  "main":"./index.cjs.js",
  "module":"./index.js",
  "types":"./index.d.ts",
  "exports":{
    ".":{
      "require":{
        "types":"./index.d.ts",
        "default":"./index.cjs.js"
      },
      "import":{
        "types":"./index.d.ts",
        "default":"./index.js"
      }
    }
  },

  "dependencies": {
    "lodash.isequal": "^4.5.0",
    "react": "^18.0.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
